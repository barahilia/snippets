alias df='df -h'
alias du='du -h'
alias free='free -h'
alias md='mkdir'
alias ls='/bin/ls --color=auto --hide=*.pyc'
alias ll='/bin/ls --color=auto -lh'
alias la='/bin/ls --color=auto -A'
alias cgrep='grep --color=force'
alias abspath='readlink -f'
alias cuts='cut -d" "'
alias wcl='wc -l'
alias counter='sort | uniq -c'
